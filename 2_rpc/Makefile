
# This is a template Makefile generated by rpcgen

# Parameters

CLIENT = fileread_client
SERVER = fileread_server

SOURCES_CLNT.c = fileread_main.c # client program
SOURCES_CLNT.h = 
SOURCES_SVC.c = fileread_proc.c # serverside implementation of call
SOURCES_SVC.h = 
SOURCES.x = fileread.x

TARGETS_SVC.c = fileread_svc.c   fileread_xdr.c 
TARGETS_CLNT.c = fileread_clnt.c   fileread_xdr.c 
TARGETS = fileread.h fileread_xdr.c fileread_clnt.c fileread_svc.c    

OBJECTS_CLNT = $(SOURCES_CLNT.c:%.c=%.o) $(TARGETS_CLNT.c:%.c=%.o)
OBJECTS_SVC = $(SOURCES_SVC.c:%.c=%.o) $(TARGETS_SVC.c:%.c=%.o)
# Compiler flags 

CFLAGS += -g -I /usr/include/tirpc/ # tirpc is no longer part of glibc, give manually
LDLIBS += -ltirpc  # link dynamic lib for rpc
RPCGENFLAGS = 

# Targets 

all : $(CLIENT) $(SERVER)

$(TARGETS) : $(SOURCES.x) 
	rpcgen $(RPCGENFLAGS) $(SOURCES.x)

$(OBJECTS_CLNT) : $(SOURCES_CLNT.c) $(SOURCES_CLNT.h) $(TARGETS_CLNT.c) 

$(OBJECTS_SVC) : $(SOURCES_SVC.c) $(SOURCES_SVC.h) $(TARGETS_SVC.c) 

$(CLIENT) : $(OBJECTS_CLNT) 
	$(LINK.c) -o $(CLIENT) $(OBJECTS_CLNT) $(LDLIBS) 

$(SERVER) : $(OBJECTS_SVC) 
	$(LINK.c) -o $(SERVER) $(OBJECTS_SVC) $(LDLIBS)

 clean:
	 $(RM) core $(TARGETS) $(OBJECTS_CLNT) $(OBJECTS_SVC) $(CLIENT) $(SERVER)

